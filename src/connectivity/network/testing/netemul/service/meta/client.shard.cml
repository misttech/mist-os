{
    include: [ "syslog/client.shard.cml" ],
    children: [
        {
            name: "sandbox",
            url: "#meta/netemul-sandbox.cm",
        },
    ],
    use: [
        {
            protocol: "fuchsia.netemul.Sandbox",
            from: "#sandbox",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.process.Launcher",
                "fuchsia.tracing.provider.Registry",
            ],
            from: "parent",
            to: "#sandbox",
        },
        {
            dictionary: "diagnostics",
            from: "parent",
            to: "#sandbox",
        },
        {
            storage: [
                "cache",
                "custom_artifacts",
                "data",
                "tmp",
            ],
            from: "parent",
            to: "#sandbox",
        },
    ],
}
