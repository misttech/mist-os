{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "skip",
            matchers: [
                // TODO(https://fxbug.dev/323052525): Support Multicast Routing.
                "multicast_forwarding_v4_ns3::missing_route",
                "multicast_forwarding_v4_ns3::unexpected_input_interface",
                "multicast_forwarding_v6_ns3::missing_route",
                "multicast_forwarding_v6_ns3::unexpected_input_interface",
                "watch_routing_events_dropped_events_v4_ns3",
                "watch_routing_events_dropped_events_v6_ns3",
                "watch_routing_events_hanging_v4_ns3",
                "watch_routing_events_hanging_v6_ns3",
                "add_multicast_route_v4_ns3::success_with_pending_packet",
                "add_multicast_route_v6_ns3::success_with_pending_packet",
            ],
        },
        {
            type: "expect_failure_with_err_logs",
            matchers: [
                // TODO(https://fxbug.dev/323052525): Support Multicast Routing.
                "add_multicast_route_v4_ns3::link_local_unicast_source_address",
                "add_multicast_route_v6_ns3::link_local_unicast_source_address",
                "del_multicast_route_v4_ns3::link_local_unicast_source_address",
                "del_multicast_route_v6_ns3::link_local_unicast_source_address",
                "get_route_stats_errors_v4_ns3::link_local_multicast_destination_address",
                "get_route_stats_errors_v4_ns3::link_local_unicast_source_address",
                "get_route_stats_errors_v4_ns3::any_source_address",
                "get_route_stats_errors_v4_ns3::no_matching_route_for_destination_address",
                "get_route_stats_errors_v4_ns3::multicast_source_address",
                "get_route_stats_errors_v4_ns3::unicast_destination_address",
                "get_route_stats_errors_v4_ns3::no_matching_route_for_source_address",
                "get_route_stats_errors_v6_ns3::link_local_unicast_source_address",
                "get_route_stats_errors_v6_ns3::no_matching_route_for_destination_address",
                "get_route_stats_errors_v6_ns3::link_local_multicast_destination_address",
                "get_route_stats_errors_v6_ns3::multicast_source_address",
                "get_route_stats_errors_v6_ns3::any_source_address",
                "get_route_stats_v4_ns3",
                "get_route_stats_errors_v6_ns3::unicast_destination_address",
                "get_route_stats_v6_ns3",
                "get_route_stats_errors_v6_ns3::no_matching_route_for_source_address",
            ],
        },
    ],
}
