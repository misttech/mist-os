{
    children: [
        {
            name: "test",
            url: "#meta/sc_capabilities_offer_and_expose.cm",
        },
        {
            name: "config",
            url: "#meta/child.cm",
        },
    ],
    offer: [
        {
            config: "fuchsia.config.MyConfig",
            from: "#config",
            to: "#test",
        },
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: "all",
        },
        {
            dictionary: "diagnostics",
            from: "parent",
            to: "all",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "#test",
        },
    ],
}
