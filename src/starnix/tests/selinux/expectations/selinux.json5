{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // TODO(http://b/330904217): update `entrypoint` expectation to pass once filesystems are labeled correctly.
                "*entrypoint*",
                "*execshare*",

                // TODO(http://b/364568874): `inherit` tests will pass once the `file_security_ops`
                // hook correctly enforces the `write` permission.
                "*inherit*",

                // TODO(http://b/330904217): update `execute_no_trans` expectation to pass once filesystems are labeled correctly.
                // keep-sorted start
                "*atsecure*",
                "*bounds*",
                "*bpf*",
                "*capable_file*",
                "*capable_net*",
                "*capable_sys*",
                "*cgroupfs_label*",
                "*checkreqprot*",
                "*dyntrace*",
                "*execute_no_trans*",
                "*fdreceive*",
                "*inet_socket*",
                "*ioctl*",
                "*key_socket*",
                "*keys*",
                "*mac_admin*",
                "*mkdir*",
                "*mmap*",
                "*module_load*",
                "*msg*",
                "*netlink_socket*",
                "*nnp_nosuid*",
                "*notify*",
                "*open*",
                "*perf_event*",
                "*readlink*",
                "*rxdir*",
                "*sem*",
                "*setattr*",
                "*shm*",
                "*stat*",
                "*sysctl*",
                "*tun_tap*",

                // keep-sorted end
                // The tests below are all explicitly expected to fail. The ones
                // containing "*" above sometimes cover more than one test.
                // keep-sorted start
                "extended_socket_class",
                "file",
                "filesystem",
                "fs_filesystem",
                "glblub",
                "overlay",
                "userfaultfd",
                "watchkey",

                // keep-sorted end
            ],
        },
        {
            type: "skip",
            matchers: [
                // Test sometimes hangs.
                "*unix_socket*",
            ],
        },
    ],
}
