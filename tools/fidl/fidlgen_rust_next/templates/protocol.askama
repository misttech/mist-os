                            {% else %}
                                ()
                            {% endif %}
                            >
                        {% else %}
                            {% if let Some(response) = method.maybe_response_payload.as_ref() %}
                                {{ context.static_wire_type(response) }}
                            {% else %}
                                ()
                            {% endif %}
                        {% endif %}
                    {% when ProtocolMethodKind::Event %}
                        {% if let Some(response) = method.maybe_response_payload.as_ref() %}
                            {{ context.static_wire_type(response) }}
                        {% else %}
                            ()
                        {% endif %}
                {% endmatch %}
